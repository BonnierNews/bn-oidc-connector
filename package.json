{
  "name": "nodestarterapp",
  "version": "0.0.3",
  "description": "Empty starter app for Bonnier News node backend apps",
  "type": "module",
  "main": "dist/index.js",
  "homepage": "https://github.com/BonnierNews/node-starterapp",
  "repository": {
    "type": "git",
    "url": "git@github.com:BonnierNews/node-starterapp.git"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "node --test --test-force-exit --test-reporter ${REPORTER:-spec} --import \"tsx/esm\" --import \"./test/setup.ts\" \"test/**/*.{test,feature}.ts\"",
    "coverage": "c8 npm test && c8 check-coverage",
    "posttest": "npm run lint",
    "lint": "eslint . --cache && npm run typecheck"
  },
  "author": "Bonnier News AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "express": "^4.20.0",
    "tsx": "^4.3.0"
  },
  "devDependencies": {
    "@bonniernews/eslint-config": "^1.1.0",
    "@bonniernews/node-test-bdd": "^0.0.4",
    "@bonniernews/tsconfig": "^0.0.2",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.1",
    "c8": "^10.1.3",
    "chai": "^4.3.7",
    "eslint": "^8.44.0",
    "mocha": "^10.2.0",
    "mocha-cakes-2": "^3.3.0",
    "nock": "^13.3.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0"
  }
}
