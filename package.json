{
  "name": "nodestarterapp",
  "version": "0.0.3",
  "description": "Empty starter app for Bonnier News node backend apps",
  "type": "module",
  "main": "dist/app.js",
  "homepage": "https://github.com/BonnierNews/node-starterapp",
  "repository": {
    "type": "git",
    "url": "git@github.com:BonnierNews/node-starterapp.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --import=tsx/esm dist/app.js",
    "dev": "node --import=tsx/esm --watch app.ts",
    "pretest": ":> logs/test.log",
    "test": "mocha",
    "posttest": "npm run lint",
    "lint": "eslint . && tsc --noEmit"
  },
  "author": "Bonnier News AB",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "@bonniernews/exp-logger": "^4.1.0",
    "exp-config": "^4.2.1",
    "exp-correlator": "^1.0.0",
    "express": "^4.18.2",
    "tsx": "^4.3.0"
  },
  "devDependencies": {
    "@bonniernews/eslint-config": "^1.1.0",
    "@bonniernews/tsconfig": "^0.0.2",
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.1",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.7",
    "eslint": "^8.44.0",
    "mocha": "^10.2.0",
    "mocha-cakes-2": "^3.3.0",
    "nock": "^13.3.1",
    "supertest": "^6.3.3",
    "typescript": "^5.3.0"
  }
}
